Page {
  background-color: #f43e34;
  height: 100%;
}

.header {
  position: relative;
  display: flex;
  padding: 20rpx 0;
  background-color: #ff5046;
  overflow: hidden;
}

.header .play-container {
  position: relative;
  display: flex;
  width: 690rpx;
  height: 690rpx;
  margin: 0 auto;
  background-color: #ff6f69;
  border-radius: 30rpx;
  overflow: hidden;
  justify-content: center;
  align-items: center;
}

.header .play-container .cover {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  width: 690rpx;
  height: 690rpx;
  border-radius: 30rpx;
  z-index: 1;
}

.header .play-container .video {
  display: flex;  
  width: 690rpx;
  height: 690rpx;
  border-radius: 30rpx;
}

.header .play-container .audio {
  display: none;
}

.header .play-container .audio-disk {
  display: flex;
  width: 300rpx;
  height: 300rpx;
  z-index: 1;
} 

.header .play-container .play-text {
  position:absolute;
  width:200rpx;
  height:30rpx;
  font-size:28rpx;
  color:#fff;
  bottom:50rpx;
  left:50%;
  transform:translateX(-50%);
  text-align:center;
}

.wrapper {
  height: 1300rpx;
}


.controller {
  position: absolute;
  bottom: 0;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 150rpx;
  background-color: #fff;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  box-sizing:border-box;
}

.play-icon {
  width: 50rpx;
  height: 50rpx;
  background: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.cover-pos {
  position: relative;
  /* bottom: 30rpx;
  left: 30rpx; */
  z-index: 2;
}

.pause-icon {
  width: 50rpx;
  height: 50rpx;
  background: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}

.progress-bar {
  position: relative;
  display: flex;
  flex-direction: row;
  height: 60rpx;
  align-items: center;
}
.progress-bar text {
  font-size: 32rpx;
}
.progress-bar .progress {
  position: relative;
  display: block;
  width: 350rpx;
  border-radius: 2rpx;
  margin: 0 20rpx 0 40rpx;

}

.header .play-container .bg {
  position: absolute;
  display: flex;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 636rpx;
  height: 222rpx;
  background: url('data:image/png;base64,');
  background-repeat: no-repeat;
  background-size: 100% 100%;
}




.rotate-ani {
  -webkit-transform-origin: 146rpx 166rpx;
  transform-origin: 146rpx 166rpx;
  -webkit-animation: loadRotate 3s linear infinite;
  -webkit-animation-fill-mode: both;
  animation: loadRotate 3s linear infinite;
  animation-fill-mode: both;
}


@-webkit-keyframes loadRotate { 
  from { 
    -webkit-transform:rotateZ(0deg); 
  } 
  to { 
    -webkit-transform:rotateZ(360deg); 
  } 
} 

@keyframes loadRotate { 
  from { 
    transform:rotateZ(0deg); 
  } 
  to { 
    transform:rotateZ(360deg); 
  } 
}

